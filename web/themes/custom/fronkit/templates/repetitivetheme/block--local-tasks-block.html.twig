{% extends "block.html.twig" %}
{#
/**
 * @file
 * Theme override for tabs.
 */
#}
{% block content %}
    {% if content %}
        <div class="container">
            <nav class="tabs local-tasks" role="navigation" aria-label="{{ 'Tabs'|t }}">
                {{ content }}
            </nav>
        </div>
    {% endif %}
{% endblock %}
